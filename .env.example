# Supabase
SUPABASE_URL=https://xyz.supabase.co
SUPABASE_SERVICE_ROLE_KEY=service-role-xxxxx

# Backend
BACKEND_BASE_URL=https://api.example.com
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000

# Redis (managed)
REDIS_URL=rediss://:password@redis-xxxx.upstash.io:6379

# Celery
CELERY_BROKER_URL=${REDIS_URL}
CELERY_RESULT_BACKEND=${REDIS_URL}

# n8n webhook secret + url
N8N_WEBHOOK_URL=https://n8n.online/webhook/<YOUR_PATH>
N8N_WEBHOOK_SECRET=supersecretstring

# Supabase anon (for client in frontend only)
NEXT_PUBLIC_SUPABASE_URL=${SUPABASE_URL}
NEXT_PUBLIC_SUPABASE_ANON_KEY=<anon_key>  # only for client usage